<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <Thickness x:Key="padDouble">20</Thickness>
    <Thickness x:Key="padDoubleTop">0 20 0 0</Thickness>
    <Thickness x:Key="padDoubleStart">20 0 0 0</Thickness>
    <Thickness x:Key="padDoubleEnd">0 0 20 0</Thickness>
    <Thickness x:Key="padDoubleBottom">0 0 0 20</Thickness>
    
    <Thickness x:Key="padNormal">10</Thickness>
    <Thickness x:Key="padNormalTop">0 10 0 0</Thickness>
    <Thickness x:Key="padNormalStart">10 0 0 0</Thickness>
    <Thickness x:Key="padNormalEnd">0 0 10 0</Thickness>
    <Thickness x:Key="padNormalBottom">0 0 0 10</Thickness>
    <Thickness x:Key="padNormalVertical">0 10</Thickness>
    <Thickness x:Key="padNormalHorizontal">10 0</Thickness>
    
    <Thickness x:Key="padSmall">5</Thickness>

    <sys:Double x:Key="wSideBarDefaultWith">220</sys:Double>
    
</ResourceDictionary>