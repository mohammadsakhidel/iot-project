<Application x:Class="TrackAdmin.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TrackAdmin"
             xmlns:viewModels="clr-namespace:TrackAdmin.ViewModels"
             xmlns:shared="clr-namespace:TrackAdmin.Shared">
    <Application.Resources>
        <ResourceDictionary>
            
            <!--Converters:-->
            
            <!--Resource Dictionaries-->
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/DefaultTheme/_Colors.xaml" />
                <ResourceDictionary Source="Resources/DefaultTheme/_Fonts.xaml" />
                <ResourceDictionary Source="Resources/DefaultTheme/_Dimensions.xaml" />
                <ResourceDictionary Source="Resources/DefaultTheme/_Templates.xaml" />
                <ResourceDictionary Source="Resources/DefaultTheme/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
